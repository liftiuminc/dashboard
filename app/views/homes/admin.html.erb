<% title "Admin Home" %>

<%= render :partial => 'shared/select_timeframe' %>

<div style="width:825px">

<% for stat in misc_stat_types do -%>
  <div style="float:left;margin-bottom:10px;margin-right:10px">
    <%= stat %>:<br />
    <a href="<%=h url_for :controller => 'charts', :action => 'misc_stat', :stat => stat %>"><img src="<%= misc_stat_url stat, @range %>" width="400" height="190"/></a>
  </div>
<% end -%>

</div>
