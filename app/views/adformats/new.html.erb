<h1>New adformat</h1>

<% form_for(@adformat) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :format_name %><br />
    <%= f.text_field :format_name %>
  </p>
  <p>
    <%= f.label :width %><br />
    <%= f.text_field :width %>
  </p>
  <p>
    <%= f.label :height %><br />
    <%= f.text_field :height %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', adformats_path %>