<% title "Beacon Aggs: Fill rate by previous attempts" %>

<p>Previous attempts count of 10 means 10 or more.</p>

<table class="record_listing" width="500" cellspacing="0" cellpadding="0"><!-- Cellspacing/paddding added for IE 6 -->
  <tr class="record_listing_header">
    <th>Network name</th>
    <th>Previous attempts</th>
    <th>Loads</th>
    <th>Rejects</th>
    <th>Fill rate</th>
    <!-- th colspan="3">Actions</th -->
  </tr>
  <% for beacon_agg in @beacon_aggs %>
  <tr class="record_row_<%= cycle("odd", "even")%>">
    <td class="record_column"><%=h beacon_agg.network_name %></td>
    <td class="record_column"><%=h beacon_agg.previous_attempts %></td>
    <td class="record_column"><%=h beacon_agg.loads %></td>
    <td class="record_column"><%=h beacon_agg.rejects %></td>
    <td class="record_column"><%=h beacon_agg.fillrate %></td>
  </tr>
  <% end %>
</table>

